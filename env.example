# Server Configuration
PORT=3000

# Vapi Configuration
# Get your API keys from https://dashboard.vapi.ai
VAPI_PRIVATE_KEY=your_private_api_key_here
VAPI_PUBLIC_KEY=your_public_api_key_here

# Main Assistant ID (Sophie is the entry point)
VAPI_ASSISTANT_ID=your_assistant_id_after_deploy

# Squad Assistant IDs (after running npm run deploy:squad)
SOPHIE_ASSISTANT_ID=sophie_id_here
MARCUS_ASSISTANT_ID=marcus_id_here
EMMA_ASSISTANT_ID=emma_id_here

# Business Configuration
BUSINESS_NAME=Pawsome Pet Grooming
MINIMUM_SETTLEMENT_PERCENTAGE=70

# Server URL (update this with your ngrok/cloudflare URL)
SERVER_URL=https://your-tunnel-url.com
